@model IrisCareSolutions.Models.Exame

<h2>Edit</h2>

<form asp-action="Edit">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Nome" class="control-label"></label>
        <input asp-for="Nome" class="form-control" />
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Descricao" class="control-label"></label>
        <input asp-for="Descricao" class="form-control" />
        <span asp-validation-for="Descricao" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Data" class="control-label"></label>
        <input asp-for="Data" class="form-control" />
        <span asp-validation-for="Data" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ResultadoData" class="control-label"></label>
        <input asp-for="ResultadoData" class="form-control" />
        <span asp-validation-for="ResultadoData" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ResultadoFileName" class="control-label"></label>
        <input asp-for="ResultadoFileName" class="form-control" />
        <span asp-validation-for="ResultadoFileName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
